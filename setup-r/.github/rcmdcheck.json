{
    "problemMatcher": [
        {
            "owner": "rcmdcheck",
            "pattern": [
                {
                    "regexp": "^(.*) [.][.][.] (ERROR|WARNING)$",
                    "severity": 2
                },
                {
                    "regexp": "^\\s*(.*)$",
                    "message": 1
                }
            ]
        }
    ]
}
